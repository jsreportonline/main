{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "run",
      "host": {
        "sourcePath": "/var/run"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "main",
      "image": "jsreportonline/main",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5488
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "run",
          "containerPath": "/run-data"
        }
      ]
    }
  ]
}