{
  "authentication" : {
    "admin": {
      "username" : "admin",
      "password": "password"
    }
  },
  "connectionString": {
    "name": "mongodb"
  },
  "httpPort": 5488,
  "httpsPort": null,
  "blobStorage": "fileSystem",
  "phantom": {
    "timeout": 60000
  },
  "loggly": {
    "level": "info",
    "subdomain": "jsreport"
  },
  "logger": {
    "ignore": ["Starting rendering request"]
  },
  "scheduling": {
        "minScheduleInterval": 300000 
    },
  "tasks": {
    "allowedModules": ["moment", "underscore"],
    "timeout": 5000,
    "nativeModules": [{
      "globalVariableName": "_",
      "module": "underscore"
    },{
      "globalVariableName": "moment",
      "module": "moment"
    }]
  },
  "scripts": {
    "allowedModules": ["moment", "underscore", "request"],
    "nativeModules": [{
      "globalVariableName": "_",
      "module": "underscore"
    },{
      "globalVariableName": "moment",
      "module": "moment"
    }],
    "timeout": 20000
  },
  "assets": {
        "allowAssetsLinkedToFiles": false
    }
}